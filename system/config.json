{
    "base_path" : "/"
    ,
    "date_format": "%d %B, %Y"
    ,
    "compiled_path" : "compiled"
    ,
    "_root" : {
        "rss" : {
            "enable" : false
        }
    }
    ,
    "compiled" : {
        "use" : "ignore"
    }
    ,
    "plugins" : {
        "use" : "ignore"
    }
    ,
    "media" : {
        "use" : "static"
    }
    ,    
    "data" : {
        "use" : "data"
    }
    ,
    "javascripts" : {
        "use" : "javascripts"
    }
    ,
    "layouts" : {
        "use" : "layouts"
    }
    ,
    "partials" : {
        "use" : "partials"
    }
    ,
    "stylesheets" : {
        "use" : "stylesheets"
    }
    ,
    "widgets" : {
        "use" : "widgets"
    }
}
